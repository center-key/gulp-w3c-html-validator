{
   "name": "gulp-w3c-html-validator",
   "version": "1.4.3",
   "license": "MIT",
   "description": "Gulp plugin to validate HTML using the W3C Markup Validation Service",
   "main": "html-validator.js",
   "repository": "github:center-key/gulp-w3c-html-validator",
   "homepage": "https://github.com/center-key/gulp-w3c-html-validator",
   "bugs": "https://github.com/center-key/gulp-w3c-html-validator/issues",
   "docs": "https://github.com/center-key/gulp-w3c-html-validator#readme",
   "author": "Callum Macrae (https://github.com/callumacrae)",
   "contributors": [
      "Dem Pilafian (https://twitter.com/dempilafian)"
   ],
   "keywords": [
      "gulp",
      "gulp-plugin",
      "html",
      "html5",
      "html-validator",
      "validate",
      "validation",
      "w3c"
   ],
   "jshintConfig": {
      "esversion": 6,
      "strict": "implied",
      "undef": true,
      "unused": true,
      "node": true,
      "mocha": true
   },
   "scripts": {
      "pretest": "jshint *.js spec",
      "test": "mocha spec/spec.js"
   },
   "dependencies": {
      "ansi-colors": "~3.2",
      "fancy-log": "~1.3",
      "plugin-error": "~1.0",
      "through2": "~3.0",
      "vinyl": "^2.2.0",
      "w3cjs": "~0.4"
   },
   "devDependencies": {
      "jshint": "~2.9",
      "mocha": "~5.2",
      "should": "~13.2"
   }
}
